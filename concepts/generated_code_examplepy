python
class Feature:
    def __init__(self, name, value):
        self.name = name
        self.value = value

    def calculate(self):
        return self.value * 2

    def __str__(self):
        return f"Feature: {self.name}, Value: {self.value}"

def process_features(features):
    results = {}
    for feature in features:
        results[feature.name] = feature.calculate()
    return results